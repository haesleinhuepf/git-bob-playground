import tkinter as tk
from tkinter import ttk

# Create main window
root = tk.Tk()
root.title("Extremely Unfriendly GUI")

# Create lots of dropdowns and spinners
for i in range(20):
    # Dropdown
    options = [f'Option {j}' for j in range(10)]
    ttk.Label(root, text=f"Dropdown {i}:").grid(row=i, column=0, padx=5, pady=2)
    ttk.Combobox(root, values=options).grid(row=i, column=1, padx=5, pady=2)
    
    # Spinner
    ttk.Label(root, text=f"Spinner {i}:").grid(row=i, column=2, padx=5, pady=2)
    ttk.Spinbox(root, from_=0, to=100).grid(row=i, column=3, padx=5, pady=2)

root.mainloop()
